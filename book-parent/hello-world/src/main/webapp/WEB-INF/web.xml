<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
    version="3.1">
    <display-name>Archetype Created Web Application</display-name>
    
    <servlet>
        <servlet-name>multiValueParameterServlet</servlet-name>
        <servlet-class>projava4webbook.helloworld.MultiValueParameterServlet</servlet-class>
        <init-param>
            <param-name>useOverseasFruits</param-name>
            <param-value>false</param-value>
        </init-param>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>multiValueParameterServlet</servlet-name>
        <url-pattern>/checkboxes</url-pattern>
    </servlet-mapping>
    
    <context-param>
        <param-name>useLists</param-name>
        <param-value>true</param-value>
    </context-param>

    <welcome-file-list>
        <welcome-file>checkboxes</welcome-file>
    </welcome-file-list>
</web-app>
